	<option value = "bingMapsAerial">Bing: Aerial</option>
	<option value = "bingMapsAerialWithLabels">Bing: Aerial w/ Labels</option>
	<option value = "bingMapsRoad">Bing: Road</option>
	<option value = "openStreetMap">OSM</option>
</select>
